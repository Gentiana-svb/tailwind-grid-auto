{
	"name": "tailwind-grid-auto",
	"version": "0.0.3",
	"type": "module",
	"main": "index.cjs",
	"types": "index.d.cts",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/Gentiana-svb/tailwind-grid-auto.git"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "svelte-kit sync && svelte-package",
		"build:site": "vite build",
